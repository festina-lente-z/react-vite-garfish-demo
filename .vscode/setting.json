{
    "editor.rulers": [
      150
    ],
    "html.format.wrapLineLength": 150,
    "html.format.endWithNewline": true,
    "explorer.confirmDragAndDrop": false,
    "html.format.wrapAttributes": "auto",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "explorer.confirmDelete": true,
    "git.ignoreLegacyWarning": true,
    "terminal.integrated.shell.osx": "/bin/zsh",
    "editor.suggestSelection": "first",
    "editor.tabSize": 2,
    "files.watcherExclude": {
      "**/.git/objects/**": true,
      "**/.git/subtree-cache/**": true,
      "**/node_modules/*/**": true,
      "**/dist/*/**": true,
      "**/coverage/*/**": true
    },
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "explicit",
      "source.fixAll.stylelint": "explicit"
    },
    "[markdown]": {
      "editor.formatOnSave": false
    },
    "[javascript]": {
      "editor.formatOnSave": false
    },
    "[json]": {
      "editor.formatOnSave": false
    },
    "[jsonc]": {
      "editor.formatOnSave": false
    },
    "[html]": {
      "editor.formatOnSave": false
    },
    "files.associations": {
      "*.conf": "shellscript",
      "*.json": "jsonc",
      ".prettierrc": "json",
      ".stylelintrc": "json",
      ".htmlhintrc": "json",
      "*.css": "tailwindcss"
    },
    "cSpell.words": [
      "ahooks",
      "antd",
      "antv",
      "bixi",
      "browserslist",
      "colour",
      "Createable",
      "cssinjs",
      "datagrand",
      "deletable",
      "dockerhub",
      "dumi",
      "echarts",
      "eqeqeq",
      "esbuild",
      "gitbeaker",
      "gitconfiglocal",
      "hoverable",
      "hyperscript",
      "Lexicals",
      "localstorage",
      "Mergeable",
      "metas",
      "middlewares",
      "mkdir",
      "modulepath",
      "modulepreload",
      "nprogress",
      "pmmmwh",
      "popconfirm",
      "popperjs",
      "portfinder",
      "prismjs",
      "Sider",
      "Splitable",
      "starport",
      "Strikethrough",
      "stylelint",
      "svgr",
      "Svgs",
      "tailwindcss",
      "taobao",
      "tinycolor",
      "twotone",
      "unhang",
      "Viewports",
      "Vite",
      "webpackbar",
      "wireframe",
      "wordcloud",
      "Yapi",
      "zrender",
      "docgen"
    ],
    "typescript.tsdk": "node_modules/typescript/lib",
    "compile-hero.disable-compile-files-on-did-save-code": true
  }
  